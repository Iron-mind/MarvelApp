{
  "expo": {
    "name": "MarvelApp",
    "slug": "marvelappv2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.example.myapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Know comics related to any character of Marvel. Go ahead. Look and enjoy",
    "githubUrl": "https://github.com/Iron-mind/MarvelApp"
  }
}
